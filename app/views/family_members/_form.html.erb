<%= form_with(model: family_member) do |form| %>
  <div class="field-group">
    <div class="label mb-2"><%= form.label :name %></div>
    <div class="input-field mb-4"><%= form.text_field :name %></div>
  </div>

  <div class="actions">
    <%= form.submit class: 'btn btn-success' %>
  </div>
<% end %>
